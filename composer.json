{
    "name": "palaknextincube/laravel-data-encryption",
    "description": "Encrypt sensitive data while maintaining searchability with Meilisearch",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Palak Rajput",
            "email": "palak.nextincube@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "ext-openssl": "*",
        "meilisearch/meilisearch-php": "^0.19.0|^0.20.0|^0.21.0|^0.22.0|^0.23.0|^0.24.0|^0.25.0|^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "PalakRajput\\DataEncryption\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PalakRajput\\DataEncryption\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "PalakRajput\\DataEncryption\\Providers\\DataEncryptionServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true

}
